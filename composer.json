{
    "name": "tradeshift/interview",
    "description": "PHP boilerplate including PSR-4 autoloading, PHPUnit test config (with separate PSR-2 namespace), and Travis CI.",
    "license": "MIT",
    "authors": [
        {
            "name": "Hieu Vo",
            "email": "hieu.vo@gmail.com"
        }
    ],
    "require": {
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0"
    },
    "autoload": {
      "psr-4": {
        "Tradeshift\\interview\\" : "src"
      },
    "files": ["config.php"]
    },
    "autoload-dev": {
      "psr-0": {
        "Tests\\": "tests/"
      }
    }
}
